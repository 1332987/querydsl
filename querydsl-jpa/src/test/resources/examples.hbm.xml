<hibernate-mapping>
	<class name="com.mysema.query.jpa.domain2.User2" table="USER">
		<id column="USER_ID" name="id" type="java.lang.Long">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">idgen</param>
				<param name="column">NEXT</param>
			</generator>
		</id>
		<property name="userName" />
		<property name="userPassword" />
		<property name="userFirstName" />
		<property name="userEmail" />
		<property name="userLastName" />
		<property name="creationDate" />
		<property name="createdBy" />
		<property name="modificationDate" />
		<property name="modifiedBy" />
		<property name="deleteDate" />
		<property name="deletedBy" />
		<set name="properties" lazy="true" inverse="true" cascade="all-delete-orphan">
			<one-to-many class="com.mysema.query.jpa.domain2.UserProp" />
		</set>
	</class>
	
	<class name="com.mysema.query.jpa.domain2.UserProp" table="USER_PROP">
		<id name="id" type="java.lang.Long">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">idgen</param>
				<param name="column">NEXT</param>
			</generator>
		</id>
		<property name="categoryName" />
		<property name="categoryDescription" />
		<property name="createdBy" />
		<property name="creationDate" />
		<property name="modifiedBy" />
		<property name="modificationDate" />
		<property name="deletedBy" />
		<property name="deleteDate" />
		<set name="childCategories">
			<one-to-many class="com.mysema.query.jpa.domain2.Category" />
		</set>
		<set name="properties" lazy="true" inverse="true" cascade="all-delete-orphan">
			<key />
			<one-to-many class="com.mysema.query.jpa.domain2.CategoryProp" />
		</set>
	</class>
	
	<class name="com.mysema.query.jpa.domain2.Category" table="CATEGORY">
		<id name="id" type="java.lang.Long">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">idgen</param>
				<param name="column">NEXT</param>
			</generator>
		</id>
		<property name="categoryName" />
		<property name="categoryDescription" />
		<property name="createdBy" />
		<property name="creationDate" />
		<property name="modifiedBy" />
		<property name="modificationDate" />
		<property name="deletedBy" />
		<property name="deleteDate" />
		<set name="childCategories">
			<one-to-many class="com.mysema.query.jpa.domain2.Category" />
		</set>
		<set name="properties" lazy="true" inverse="true" cascade="all-delete-orphan">
			<one-to-many class="com.mysema.query.jpa.domain2.CategoryProp" />
		</set>
	</class>
	
	<class name="com.mysema.query.jpa.domain2.CategoryProp" table="CATEGORY_PROP">
		<id name="id" type="java.lang.Long">
			<generator class="org.hibernate.id.TableHiLoGenerator">
				<param name="table">idgen</param>
				<param name="column">NEXT</param>
			</generator>
		</id>
		<property name="categoryId" />
		<property name="propName" />
		<property name="propValue" />
	</class>
	
	<class name="com.mysema.query.jpa.domain2.Document2" table="DOCUMENT">
		<id name="id" type="java.lang.Long">
			<generator class="sequence" />
		</id>
		<property name="documentTitle" />
		<property name="documentBody" />
		<property name="documentSummary" />
		<property name="createdBy" />
		<property name="creationDate" />
		<property name="modifiedBy" />
		<property name="modificationDate" />
		<property name="deletedBy" />
		<property name="deletedDate" />
		<property name="documentVersion" />
		<property name="documentStatus" />
		<property name="entryId" />
	</class>
	
	<class name="com.mysema.query.jpa.domain2.DocumentProp" table="DOCUMENT_PROP">
		<id name="id" type="java.lang.Long">
			<generator class="sequence" />
		</id>
		<property name="documentId" not-null="true" />
		<property name="propName" not-null="true" />
		<property name="propValue" />
		<property name="propValueDetails" />
	</class>
	
	<class name="com.mysema.query.jpa.domain2.Parent">
		<id name="id" type="java.lang.Long">
			<generator class="sequence" />
		</id>
		<property name="name"/>
		<component name="child">
			<property name="childName"/>
		</component>	
	</class>	
	
</hibernate-mapping>